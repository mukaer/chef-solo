login: &login
  adapter:  <%= @adpter %>
  host:     <%= @host %>
  username: <%= @username %>
  password: <%= @password %>
  encoding: <%= @encoding %>
 
 
development:
  database: <%= @db_dev %>
  <<: *login
 
test:
  database: <%= @db_test %>
  <<: *login
 
production:
  database: <%= @db_pro %>
  <<: *login
